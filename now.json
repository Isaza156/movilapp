{
    "version":2,
    "builds":[
        {
            "src": "/*.js",
            "use": "@now/node-server"
        }
    ],
    "routes":[
        {
            "src":"/",
            "dest":"/AppRoute.js",
            "methods":["GET","POST","PUT","DELETE"]
        },
        {
            "src": "/(.*)",
            "dest": "/AppRoute.js"
        }
    ]
}